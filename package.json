{
  "name": "rototill",
  "version": "0.6.2",
  "description": "A route building utility for Express to help with validation, typings, and OpenAPI specs for REST APIs",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "type": "module",
  "types": "./lib/cjs/index.js",
  "repository": {
    "url": "https://github.com/Drewpom/rototill"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --project tsconfig.build.json",
    "build:cjs": "tsc --project tsconfig.build.cjs.json",
    "run-example": "ts-node-esm ./example/index.ts",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prepublish": "./scripts/fixBuild.sh"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.10",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "ajv": "^8.11.2",
    "body-parser": "^1.20.1",
    "eslint": "^8.30.0",
    "express": "^4.18.2",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "ajv": "^8.11.2"
  },
  "files": [
    "/lib"
  ]
}
